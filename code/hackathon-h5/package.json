{
  "private": true,
  "scripts": {
    "start": "cross-env MetaForestEnv=local umi dev",
    "local": "npm run start",
    "build:dev": "cross-env MetaForestEnv=dev  umi build",
    "build:test": "cross-env MetaForestEnv=test  umi build",
    "build:prod": "cross-env MetaForestEnv=prod  umi build",
    "build": "cross-env MetaForestEnv=prod CDN=yes  umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "antd-mobile": "^5.12.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "decimal.js": "^10.3.1",
    "ethers": "^5.6.8",
    "mockjs": "^1.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "react": "17.x",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "17.x",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.23",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
